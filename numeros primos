#include <stdio.h>
#include <iostream>
using namespace std;
int main() {
    int primos, n, d, primo;

   printf("CuÃ¡ntos primos: ");
    scanf("%d", &primos);

   n = 2;
    while (primos > 0) {

       primo = 1;
        for (d = 2; d < n; ++d) {
            if (n % d == 0) {
                primo = 0;
                break;
            }
        }
       if (primo) {
            printf("%d ", n);
            primos--;
        }
        n++;
    }
    printf("\n");
}
